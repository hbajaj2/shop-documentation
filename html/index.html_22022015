  <script>
  $(function() {
    $( "#sortable1, #sortable2" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();
    $( "#sortable3, #sortable4" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();
    $( "#sortable5, #sortable6" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();
    $( "#sortable7, #sortable8" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();
    $( "#sortable9, #sortable10" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();
    $( "#sortable11, #sortable12" ).sortable({
      connectWith: ".list-group"
    }).disableSelection();    
  });

  function selectAllProd(){
    if($('#prodall').prop('checked')) {
      $('#sortable1 #ui-state-default').each(function(i,v) {
         $('#sortable2').append(this);
      });
    }else{
      $('#sortable2 #ui-state-default').each(function(i,v) {
         $('#sortable1').append(this);
      });
    }
  }
  
  function selectAllBurst(){
    if($('#burstall').prop('checked')) {
      $('#sortable3 #ui-state-default').each(function(i,v) {
         $('#sortable4').append(this);
      });
    }else{
      $('#sortable4 #ui-state-default').each(function(i,v) {
         $('#sortable3').append(this);
      });
    }
  }

  function selectAllSit3(){
    if($('#sit3all').prop('checked')) {
      $('#sortable5 #ui-state-default').each(function(i,v) {
         $('#sortable6').append(this);
      });
    }else{
      $('#sortable6 #ui-state-default').each(function(i,v) {
         $('#sortable5').append(this);
      });
    }
  }

  function selectAllSit3b(){
    if($('#sit3ball').prop('checked')) {
      $('#sortable7 #ui-state-default').each(function(i,v) {
         $('#sortable8').append(this);
      });
    }else{
      $('#sortable8 #ui-state-default').each(function(i,v) {
         $('#sortable7').append(this);
      });
    }
  }

  function selectAllDR1(){
    if($('#dr1all').prop('checked')) {
      $('#sortable9 #ui-state-default').each(function(i,v) {
         $('#sortable10').append(this);
      });
    }else{
      $('#sortable10 #ui-state-default').each(function(i,v) {
         $('#sortable9').append(this);
      });
    }
  }

  function selectAllDR2(){
    if($('#dr2all').prop('checked')) {
      $('#sortable11 #ui-state-default').each(function(i,v) {
         $('#sortable12').append(this);
      });
    }else{
      $('#sortable12 #ui-state-default').each(function(i,v) {
         $('#sortable11').append(this);
      });
    }
  }

  function submitForm(){

    var counter=0;

    //Prod Hall checks
    if(!$('#burstfirst').prop('checked')) {

        $('#sortable2 input[type="hidden"]').each(function(i, v) {

          $(this).val(i+1);
          counter = i+1;
        });

        $('#sortable2 input[type="checkbox"]').each(function(i, v) {

          $(this).prop('checked', true);

        });

        $('#sortable4 input[type="hidden"]').each(function(i, v) {

          $(this).val(counter+i+1);

        });


        $('#sortable4 input[type="checkbox"]').each(function(i, v) {

          $(this).prop('checked', true);

        });
    }else{

        $('#sortable4 input[type="hidden"]').each(function(i, v) {

          $(this).val(i+1);
          counter = i+1;
        });


        $('#sortable4 input[type="checkbox"]').each(function(i, v) {

          $(this).prop('checked', true);

        });

        $('#sortable2 input[type="hidden"]').each(function(i, v) {

          $(this).val(counter+i+1);
         
        });

        $('#sortable2 input[type="checkbox"]').each(function(i, v) {

          $(this).prop('checked', true);

        });
    }//Prod Hall Order

    counter=0;

    if(!$('#sit3bfirst').prop('checked')) {

      $('#sortable6 input[type="hidden"]').each(function(i, v) {

        $(this).val(i+1);
        counter = i+1;
      });

      $('#sortable6 input[type="checkbox"]').each(function(i, v) {

        $(this).prop('checked', true);

      });

      $('#sortable8 input[type="hidden"]').each(function(i, v) {

        $(this).val(counter+i+1);

      });

      $('#sortable8 input[type="checkbox"]').each(function(i, v) {

        $(this).prop('checked', true);

      });
  }else{
      $('#sortable8 input[type="hidden"]').each(function(i, v) {

        $(this).val(i+1);
        counter = i+1;
      });

      $('#sortable8 input[type="checkbox"]').each(function(i, v) {

        $(this).prop('checked', true);

      });

      $('#sortable6 input[type="hidden"]').each(function(i, v) {

        $(this).val(counter+i+1);
      });

      $('#sortable6 input[type="checkbox"]').each(function(i, v) {

        $(this).prop('checked', true);

      });
  }
    counter=0;

  if(!$('#dr2first').prop('checked')) {

    $('#sortable10 input[type="hidden"]').each(function(i, v) {

      $(this).val(i+1);
      counter = i+1;
    });

    $('#sortable10 input[type="checkbox"]').each(function(i, v) {

      $(this).prop('checked', true);

    });

    $('#sortable12 input[type="hidden"]').each(function(i, v) {

      $(this).val(counter+i+1);

    });

    $('#sortable12 input[type="checkbox"]').each(function(i, v) {

      $(this).prop('checked', true);

    });
  }else{
    $('#sortable12 input[type="hidden"]').each(function(i, v) {

      $(this).val(i+1);
      counter = i+1;
    });

    $('#sortable12 input[type="checkbox"]').each(function(i, v) {

      $(this).prop('checked', true);

    });

    $('#sortable10 input[type="hidden"]').each(function(i, v) {

      $(this).val(counter+i+1);
      
    });

    $('#sortable10 input[type="checkbox"]').each(function(i, v) {

      $(this).prop('checked', true);

    });


  }

    document.forms['test'].submit();
  }
  </script>
