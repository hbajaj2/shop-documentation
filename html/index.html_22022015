      $(document).ready(function() {
          $("input[name$='ENV']").click(function() {
                      var inputid = $(this).attr('id');
                      $("input:checkbox").attr('checked', false);
                      $("div.desc").hide();
                      $("#"+inputid + "div").show();
          });
      });
    </script>
